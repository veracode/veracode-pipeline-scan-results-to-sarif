{
  "name": "convert_veracode_pipeline_scan_result_to_sarif_2.1.0",
  "version": "1.0.0",
  "description": "Convert veracode pipeline scan JSON output to SARIF format",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/",
    "build": "npm run clean && ncc build ./src/action.ts --license license",
    "test": "ava --verbose",
    "preinstall": "npx npm-force-resolutions"
  },
  "resolutions": {
    "lodash": "4.17.21",
    "normalize-url": "4.5.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.21",
    "@types/sarif": "^2.1.3",
    "@vercel/ncc": "^0.27.0",
    "ava": "^3.15.0",
    "commander": "^7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@actions/core": "^1.9.1"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
